    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "offline-plugin": "4.6.1",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.3",
    "postcss-variable-media": "0.2.1",
    "prettier": "0.22.0",
    "pug": "2.0.0-beta11",
    "pug-loader": "2.3.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-svg-loader": "1.1.1",
