    "lint:css": "csscomb -lv src/components/**/*.css",
    "lint:js": "eslint '{config,src,test}/**/*.js'",
    "format:js": "prettier --single-quote --write '{config,src,test}/**/*.js'",
    "travis": "yarn run format && yarn run lint"
  },
  "repository": "https://github.com/albertfdp/nandez.cat",
  "author": "Albert Fern√°ndez <albert@nandez.cat>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.3.2",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.2.2",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "classnames": "^2.2.5",
    "css-loader": "0.26.2",
    "csscomb": "4.0.1",
    "eslint": "3.18.0",
    "eslint-config-prettier": "1.5.0",
    "eslint-config-standard": "7.1.0",
    "eslint-config-standard-react": "4.3.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-promise": "3.5.0",
